{
  "expo": {
    "name": "bus",
    "slug": "bus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/bus.png",
    "scheme": "bus",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.bus"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/bus.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.bus"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "react-native-android-widget",
        {
          "widgets": [
            {
              "name": "BusETAWidget",
              "label": "Bus ETA Widget",
              "minWidth": "60dp",
              "minHeight": "60dp",
              "maxWidth": "600dp",
              "maxHeight": "400dp",
              "minResizeWidth": "60dp",
              "minResizeHeight": "60dp",
              "targetCellWidth": 2,
              "targetCellHeight": 1,
              "description": "Display bus route ETAs (272P, 272X)",
              "previewImage": "./assets/images/widget_preview.png",
              "updatePeriodMillis": 300000,
              "resizeMode": "horizontal|vertical"
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ae7cf931-6f35-433e-a350-2ce5f926ed6e"
      }
    },
    "owner": "josefgst"
  }
}
